{
  "expo": {
    "name": "snapbook",
    "slug": "snapbook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.kichu12348.snapbook"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6213b132-f271-4e91-a631-9dfea3796ead"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6213b132-f271-4e91-a631-9dfea3796ead"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/allspice.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Allspice",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/allspice.ttf",
                    "weight": 400
                  }
                ]
              }
            ]
          }
        }
      ]
    ]
  }
}
